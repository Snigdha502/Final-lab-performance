ckdir problem2.txt
couch problem2.txt
cano problem2.txt
chmod +x problem2.sh
./ problem2.sh

#!/bin/bash

echo "Enter starting year:"
read s

echo "Enter ending year:"
read e

c=0

for ((y=s; y<=e; y++))
do
  if (( (y % 4 == 0 && y % 100 != 0) || y % 400 == 0 ))
  then
    echo $y
    c=$((c + 1))
  fi
done

echo "Total leap years: $c"



